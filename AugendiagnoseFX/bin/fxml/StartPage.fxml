<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<GridPane fx:controller="de.eisfeldj.augendiagnosefx.Main$StartController"
	xmlns:fx="http://javafx.com/fxml" alignment="CENTER" hgap="10" vgap="10"
	styleClass="root">
	<padding>
		<Insets top="25" right="25" bottom="25" left="25" />
	</padding>

	<Button text="%button_organize_new_photos" GridPane.columnIndex="0"
		GridPane.rowIndex="0" onAction="#handleButtonOrganize" />

	<Button text="%button_display_pictures" GridPane.columnIndex="1"
		GridPane.rowIndex="0" onAction="#handleButtonDisplay" />

</GridPane>